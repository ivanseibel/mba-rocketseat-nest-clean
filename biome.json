{
	"$schema": "https://biomejs.dev/schemas/1.9.4/schema.json",
	"javascript": {
		"parser": {
			"unsafeParameterDecoratorsEnabled": true
		}
	},
	"linter": {
		"enabled": true,
		"rules": {
			"style": {
				"useImportType": "off"
			},
			"complexity": {
				"noUselessConstructor": "off"
			}
		}
	}
}
